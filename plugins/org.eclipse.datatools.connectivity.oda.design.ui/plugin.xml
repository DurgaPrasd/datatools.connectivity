<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.0"?>
<plugin
   id="org.eclipse.datatools.connectivity.oda.design.ui"
   name="%plugin.name"
   version="3.0.0"
   provider-name="Eclipse.org">

   <runtime>
      <library name="odadesignui.jar">
         <export name="*"/>
      </library>
   </runtime>
   <requires>
      <import plugin="org.eclipse.core.runtime"/>
      <import plugin="org.eclipse.ui"/>
      <import plugin="org.eclipse.datatools.connectivity.oda.profile"/>
      <import plugin="org.eclipse.datatools.connectivity.ui"/>
      <import plugin="org.eclipse.datatools.connectivity.oda"/>
      <import plugin="org.eclipse.datatools.connectivity"/>
      <import plugin="org.eclipse.datatools.connectivity.oda.design"/>
   </requires>
   <extension-point id="dataSource" name="Open Data Access Designer UI Extension Point" schema="schema/datasource.exsd"/>

</plugin>
