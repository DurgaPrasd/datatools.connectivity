<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.0"?>
<plugin
   id="org.eclipse.datatools.connectivity.dbdefinition.mysql"
   name="%pluginName"
   version="0.7.0"
   provider-name="%providerName">
   
   <extension
         point="org.eclipse.datatools.connectivity.sqm.core.databaseDefinition">
      <definition
            version="1.0"
            product="Generic JDBC"
            productDisplayString="%1.0_ProductString"
            versionDisplayString="%1.0_VersionString">
      </definition>
   </extension>

   <extension
         point="org.eclipse.emf.ecore.uri_mapping">
      <mapping
            source="Generic JDBC_1.0.xmi"
            target="runtime/vendors/Generic JDBC_1.0/Generic JDBC_1.0.xmi">
      </mapping>
   </extension>
   
</plugin>
